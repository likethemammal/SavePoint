{
	"name": "SavePoint",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "This extension was created with the awesome extensionizr.io",
	"homepage_url": "http://extensionizr.com",
	"browser_action": {
		"default_icon":  "icons/icon16inactive.png",
		"default_title": "Save Point"     // optional; shown in tooltip
	},
	"icons": {
		  "48": "icons/icon48g.png"
		, "128": "icons/icon128.png"
	},
	"default_locale": "en",
	"background": {
		"page": "src/bg/background.html",
		"persistent": true
	},
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	],
	  "content_scripts": [
		{
			"matches": ["http://www.google.com/*"],
			"js": ["jquery.js"]
		}
	],
	"web_accessible_resources": [
		"savepoint.js",
		"jquery.js",
		"jquery.nearest.min.js",
		"src/bg/savepoint.css",
		"src/bg/clearData.js",
		"src/bg/onActivate.js",
		"src/bg/checkTab.js", 
		"icons/icon48g.png",
		"icons/icon48y.png"
	]
}